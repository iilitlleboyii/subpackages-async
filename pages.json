{
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    },
    {
      "path": "pages/home/index",
      "style": {
        "navigationBarTitleText": "uni-app",
        "componentPlaceholder": {
          "zero-loading": "view"
        }
      }
    }
  ],
  "subPackages": [
    {
      "root": "packages",
      "pages": [
        {
          "path": "index/index",
          "style": {
            "navigationBarTitleText": "uni-app"
          }
        }
      ]
    }
  ],
  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": ["packages"]
    }
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "uniIdRouter": {},
  "easycom": {
    "autoscan": true,
    "custom": {}
  }
}
